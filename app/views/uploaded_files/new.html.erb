<h1>Upload</h1>

<% content_for :action_left do %>
  <% if @lecture %>
    <%= link_to (fa_icon 'arrow-left', text: 'Back'), class: "btn btn-outline-primary" %>
  <% else %>
    <%= link_to (fa_icon 'arrow-left', text: 'Back'), (@lecture ? course_lecture_path(@course, @lecture) : course_path(@course)), class: "btn btn-outline-primary" %>
  <% end %>
<% end %>

<%= render 'form', uploaded_file: @uploaded_file %>

